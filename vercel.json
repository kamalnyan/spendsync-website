{
  "version": 2,
  "buildCommand": "./vercel.build.sh",
  "outputDirectory": "dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/(.*)", "status": 404, "dest": "/fallback.html" }
  ],
  "errorDocument": "fallback.html"
} 